.tDHeader {
  height: 400rpx;
  width: 100%;
  overflow: hidden;
  position: relative;
}

.tDHeader image {
  width: 100%;
  position: relative;
}

.tDHeader .topName{
  position:absolute;
  z-index: 11;
  left: 30rpx;
  bottom: 100rpx;
  font-size: 80rpx;
  color: rgb(71, 84, 109);
  background: rgba(255, 255, 255, 0.5);
  padding:10rpx 15rpx;
  border-radius: 10rpx;
}

.listHeader {
  display: flex;
  justify-content: space-between;
  height: 80rpx;
  border-radius: 15rpx;
  position: relative;
  top: -20rpx;
  z-index: 10;
  background-color: #fff;
}

.listHeader view {
  display: flex;
  font-size: 30rpx;
  align-items: center;
}

.listHeader .left {
  margin-left: 20rpx;
  display: flex;
  align-items: center;
}

.listHeader .right {
  margin-right: 20rpx;
  display: flex;
  align-items: center;
}

.listHeader .iconfont {
  margin-right: 10rpx;
}

.listHeader .icon-liebiao {
  font-size: 40rpx;
}

.scrollItem {
  display: flex;
  padding: 0 15rpx;
}

.scrollItem image {
  width: 80rpx;
  border-radius: 10rpx;
}

.scrollItem view {
  display: flex;
  flex-direction: column;
  margin-bottom: 15rpx;
}

.scrollItem text {
  line-height: 40rpx;
  font-size: 26rpx;
  max-width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 10rpx;
}

.scrollItem .iconfont {
  position: absolute;
  height: 80rpx;
  width: 80rpx;
  right: 15rpx;
  text-align: right;
  line-height: 80rpx;
}

.listBody .acd {
  height: calc(100vh - 180rpx);
}